{
  "name": "open-api",
  "description": "freeCodeCamp's open-api Intiative",
  "license": "BSD-3-Clause",
  "author": "freeCodeCamp <team@freecodecamp.org> (https://freecodecamp.org)",
  "homepage": "https://github.com/freeCodeCamp/open-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freeCodeCamp/open-api.git"
  },
  "bugs": {
    "url": "https://github.com/freeCodeCamp/open-api/issues"
  },
  "version": "0.0.1",
  "main": "handler.js",
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "commitlint -e",
    "deploy-dev": "serverless --stage=dev deploy",
    "deploy-prod": "serverless --stage=prod deploy",
    "encode-file-contents": "node scripts/base64encode.js",
    "format": "prettier --write es5 './**/*.{js,json}' && yarn lint",
    "generate-auth-header": "node scripts/generateHeader",
    "lint": "eslint ./**/*.js --fix",
    "prepare-production": "snyk protect",
    "prepublishOnly": "yarn snyk-protect",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "start":
      "cross-env DEBUG=fcc:* nodemon node_modules/serverless/bin/serverless offline start --skipCacheInvalidation --stage dev",
    "test": "cross-env JWT_CERT=test jest --runInBand  --verbose --silent",
    "test-ci": "cross-env JWT_CERT=test jest --runInBand --verbose --silent",
    "test-update-snapshot":
      "cross-env JWT_CERT=test jest --runInBand --verbose --updateSnapshot"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "apollo-errors": "^1.9.0",
    "apollo-server-lambda": "^1.3.4",
    "auth0-js": "^9.5.0",
    "aws-lambda": "^0.1.2",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "dotenv": "^5.0.1",
    "graphql": "0.13.2",
    "graphql-playground-middleware-lambda": "1.4.4",
    "graphql-tools": "^2.24.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.4",
    "merge-graphql-schemas": "1.5.1",
    "moment": "^2.20.1",
    "mongoose": "^5.0.12",
    "serverless": "^1.26.1",
    "serverless-offline": "^3.20.0",
    "serverless-offline-scheduler": "^0.3.3",
    "serverless-webpack": "^5.1.1",
    "snyk": "^1.73.0",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "webpack": "^4.5.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/travis-cli": "^6.1.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.4",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-freecodecamp": "^1.1.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^23.0.0",
    "jest-cli": "^23.2.0",
    "lint-staged": "^7.0.4",
    "mongodb-memory-server": "^1.7.3",
    "nodemon": "^1.17.3",
    "prettier": "^1.11.1",
    "prettier-package-json": "^1.5.1",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-plugin-notification-ojongerius": "^1.3.1",
    "sinon": "^4.5.0",
    "sinon-stub-promise": "^4.0.0",
    "source-map-support": "^0.5.4"
  },
  "keywords": ["open-api"],
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["/node_modules/", "./dist"],
    "coverageReporters": ["lcov", "html"],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"],
    "package.json": ["prettier-package-json --write", "git add"]
  },
  "snyk": true
}
